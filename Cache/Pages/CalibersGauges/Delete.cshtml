@page "{id}"
@model Cache.Pages.CalibersGauges.DeleteModel

@{
    ViewData["Title"] = "Delete Caliber/Gauge";
}

<h1>@ViewData["Title"]</h1>

<div>
    <partial name="_Detail" model="@Model.CaliberGauge" />

    <p>Are you sure you want to delete this?</p>
    
    <form method="post">
        <input type="hidden" asp-for="CaliberGauge.Id" />
        <input type="submit" value="Delete" class="btn btn-danger" />
        <a class="btn btn-secondary" asp-page="./Index">Cancel</a>
    </form>
</div>
